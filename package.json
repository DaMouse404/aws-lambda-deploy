{
  "name": "dpl",
  "version": "1.0.1",
  "description": "deploy your lambda function to AWS the quick & easy way.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 30000",
    "coverage":"node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcov -- -R spec --timeout 30000",
    "s3-deploy": "gulp deploy",
    "cov1":"node_modules/.bin/istanbul cover node_modules/.bin/_mocha test/copy_files.test.js --report lcov -- -R spec",
    "test1":"node_modules/.bin/mocha test/copy_files.test.js"
  },
  "files_to_pack": [ "package.json", "index.js", "lib/" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/aws-lambda-deploy.git"
  },
  "keywords": [
    "AWS",
    "Amazon",
    "Lambda",
    "Deploy",
    "Function"
  ],
  "author": "Dream Team @ Numo Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/aws-lambda-deploy/issues"
  },
  "homepage": "https://github.com/numo-labs/aws-lambda-deploy#readme",
  "devDependencies": {
    "aws-lambda-test-utils": "^1.0.2",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "semistandard": "^7.0.5"
  },
  "dependencies": {
    "aws-sdk": "^2.2.29"
  },
  "semistandard": {
    "globals": [
      "describe",
      "it"
    ],
    "rules": {
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclaration": true
          }
        }
      ],
      "brace-style": 0,
      "no-trailing-spaces": 0
    }
  }
}
